<!-- Layout of the list of all google shopping data feeds -->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <head>
        <link src="Wyomind_Framework::js/progress.js"/>
        <link src="Wyomind_Framework::css/progress.css"/>
        <css src="Wyomind_OrderUpdater::css/grid.css"/>

    </head>
    <body>
        <referenceContainer name="content">
            <block class="Wyomind\OrderUpdater\Block\Adminhtml\Profiles" name="orderupdater_profiles_index"/>
            <block template="Wyomind_OrderUpdater::index/report.phtml" class="Magento\Backend\Block\Template"
                   name="wyomind_orderupdater_report"/>
            <block class="Magento\Backend\Block\Template" name="orderupdater_profiles_import"
                   template="Wyomind_OrderUpdater::index/import.phtml"/>
        </referenceContainer>
    </body>
</page>