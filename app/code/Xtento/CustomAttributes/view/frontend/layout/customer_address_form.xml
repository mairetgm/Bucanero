<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="customer_address_edit" remove="true" />
        <referenceContainer name="content">
            <block class="Xtento\CustomAttributes\Block\Address\Edit" name="customer_ca_address_edit" template="Xtento_CustomAttributes::address/edit.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>

