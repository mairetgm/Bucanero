<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="customer_account_dashboard_info">
            <action  method="setTemplate">
                <argument name="template" xsi:type="string">Xtento_CustomAttributes::customer/dashboard/info.phtml</argument>
            </action>
            <block class="Xtento\CustomAttributes\Block\Customer\Dashboard\CustomerAttributes" name="customer_attributes" template="Xtento_CustomAttributes::customer/dashboard/customer_attributes.phtml"/>
        </referenceBlock>
    </body>
</page>

