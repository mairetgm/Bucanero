<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Api/etc/extension_attributes.xsd">
    <extension_attributes for="Magento\Quote\Api\Data\AddressInterface">
        <attribute code="customer_address" type="Xtento\CustomAttributes\Model\Api\Data\AttributeData"/>
        <attribute code="customer" type="Xtento\CustomAttributes\Model\Api\Data\AttributeData"/>
        <attribute code="order_field" type="Xtento\CustomAttributes\Model\Api\Data\AttributeData"/>
    </extension_attributes>
    <extension_attributes for="Magento\Sales\Api\Data\OrderAddressInterface">
        <attribute code="customer" type="Xtento\CustomAttributes\Model\Api\Data\AttributeData"/>
        <attribute code="customer_address" type="Xtento\CustomAttributes\Model\Api\Data\AttributeData"/>
    </extension_attributes>
    <extension_attributes for="Magento\Sales\Api\Data\OrderInterface">
        <attribute code="order_field" type="Xtento\CustomAttributes\Model\Api\Data\OrderAdditionalInfo[]"/>
    </extension_attributes>
</config>
